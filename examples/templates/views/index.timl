section.container > div.row > div.col-5.mx-auto
    h3.appName: $app.appName
        if $app.production == true:
            span status="production": "(P)"
        else:
            span status="development": "(D)"
    h3: "Keywords:"
    ul.keywords
        for $keyword in $app.keywords:
            li.item > a.tag href="#": $keyword